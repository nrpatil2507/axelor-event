<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">

  <form name="address-form" title="Address" model="com.axelor.event.db.Address">
  <panel title="Address Detail">
    <field name="flat"/>
    <field name="street"/>
    <field name="landMark"/>
    <field name="city"/>
    <field name="country"/>
    <field name="fullName"/>
  </panel>
</form>

<grid name="address-grid" title="Addresses" model="com.axelor.event.db.Address">
  <field name="flat"/>
  <field name="street"/>
  <field name="landMark"/>
  <field name="city"/>
  <field name="country"/>
  <field name="fullName"/>
</grid>

  </object-views>
